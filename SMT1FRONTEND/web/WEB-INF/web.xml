<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>startSession</servlet-name>
        <servlet-class>za.ac.tut.serv.startSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MidSession</servlet-name>
        <servlet-class>za.ac.tut.serv.MidSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>endSession</servlet-name>
        <servlet-class>za.ac.tut.serv.endSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewAllstast</servlet-name>
        <servlet-class>za.ac.tut.serv.viewAllstast</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>startSession</servlet-name>
        <url-pattern>/startSession.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MidSession</servlet-name>
        <url-pattern>/MidSession.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>endSession</servlet-name>
        <url-pattern>/endSession.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewAllstast</servlet-name>
        <url-pattern>/viewAllstast.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
